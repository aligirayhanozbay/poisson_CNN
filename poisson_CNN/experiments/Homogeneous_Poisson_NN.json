{
    "model":{
	"data_format":"channels_first",
	"ndims":2,
	"use_batchnorm":false,
	"pre_bottleneck_convolutions_config":{
            "filters": [2,3,4,6],
            "kernel_sizes": [13,11,9,7],
            "padding_mode": "SYMMETRIC",
            "conv_activation": "tf.nn.leaky_relu",
            "dense_activations": "tf.nn.leaky_relu",
            "use_bias": false,
            "pre_output_dense_units": [8,16],
	    "kernel_initializer":"ones",
	    "bias_initializer":"ones",
	    "kernel_regularizer":"l2",
	    "bias_regularizer":"l2"
	},
	"bottleneck_config":{
            "downsampling_factors": [1,2,3,4,8,16],
            "upsampling_factors": [1,2,3,4,8,16],
            "filters": 6,
            "conv_kernel_sizes": [7,7,7,7,5,5],
            "deconv_kernel_sizes": [3,2,3,4,8,16],
            "n_convs": [2,2,2,2,2,2],
            "conv_padding_mode": "SYMMETRIC",
            "conv_conv_activation": "tf.nn.leaky_relu",
            "conv_dense_activation": "tf.nn.leaky_relu",
            "conv_pre_output_dense_units": [8,16],
            "conv_use_bias": false,
	    "conv_initializer_constraint_regularizer_options":{
		"kernel_initializer":"zeros",
		"bias_initializer":"zeros",
		"kernel_regularizer":"l2",
		"bias_regularizer":"l2"
	    },
            "deconv_conv_activation": "tf.nn.leaky_relu",
            "deconv_dense_activation": "tf.nn.leaky_relu",
            "deconv_pre_output_dense_units": [8,16],
            "deconv_use_bias": false,
	    "deconv_initializer_constraint_regularizer_options":{
		"kernel_initializer":"zeros",
		"bias_initializer":"zeros",
		"kernel_regularizer":"l2",
		"bias_regularizer":"l2"
	    },
            "use_resnet": true,
            "conv_downsampling_kernel_sizes": [3,2,3,4,8,16]
        },
	"final_convolutions_config":{
            "filters": [6,4,2,1,1],
            "kernel_sizes": [5,5,3,3,1],
            "padding_mode": "SYMMETRIC",
            "conv_activation": "tf.nn.leaky_relu",
            "dense_activations": "tf.nn.leaky_relu",
            "use_bias": true,
            "pre_output_dense_units": [8,16],
	    "kernel_initializer":"zeros",
	    "bias_initializer":"zeros",
	    "kernel_regularizer":"l2",
	    "bias_regularizer":"l2"
        },
	"output_scaling":{
	    "max_domain_size_squared":true
	}
    },
    "training":{
	"n_epochs": 999,
	"optimizer": "adam",
	"optimizer_parameters":{
	    "learning_rate": 1e-2,
	    "amsgrad": true
	},
	"loss_parameters":{
	    "ndims": 2,
	    "integral_loss_weight":1.0,
	    "data_format":"channels_first",
	    "integral_loss_config":{
		"n_quadpts":20,
		"Lp_norm_power":2
	    },
	    "physics_informed_loss_weight":1.0,
	    "physics_informed_loss_config":{
		"stencil_sizes":[5,5],
		"orders":2,
		"normalize":false
	    }
	}
    },
    "dataset":{
	"batch_size":10,
	"batches_per_epoch":200,
	"output_size_range":[200,300],
	"fourier_coeff_grid_size_range":[5,15],
	"taylor_degree_range":[3,8],
	"grid_spacings_range":[1e-3,1e-1],
	"ndims":2,
	"homogeneous_bc":true,
	"return_rhses":true,
	"return_boundaries":false,
	"return_dx":true,
	"normalizations":{"rhs_max_magnitude":true}
    }
}
